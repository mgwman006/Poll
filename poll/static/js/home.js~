                 $("#ch6").show()
                 $("#ch5").show()
                 $("#ch4").show()
                 $("#ch3").show()
                 $("#ch2").show()
                 $("#ch1").show()
                 break;

                 
            case 9:
            
                 $("#ch10").hide()
