                 $("#ch6").hide()
                 $("#ch5").hide()
                 $("#ch4").hide()
                 $("#ch3").show()
                 $("#ch2").show()
                 $("#ch1").show()
                 break;

            case 4:
            
                 $("#ch10").hide()
                 $("#ch9").hide()
                 $("#ch8").hide()
                 $("#ch7").hide()
                 $("#ch6").hide()
                 $("#ch5").hide()
                 $("#ch4").show()
                 $("#ch3").show()
                 $("#ch2").show()
                 $("#ch1").show()
                 break;
 
            case 5:
            
