<script>

    function getSelectValue()
    {

        var x = document.getElementById("inputGroupSelect01").value;

        switch(x)
        {
            case 1:
                 $("#ch10").hide()
                 $("#ch9").hide()
                 $("#ch8").hide()
                 $("#ch7").hide()
                 $("#ch6").hide()
                 $("#ch5").hide()
                 $("#ch4").hide()
                 $("#ch3").hide()
                 $("#ch2").hide()
                 $("#ch1").show()
